{
  "name": "thapp/jmg-http-foundation",
  "description": "Symfony compatible image response",
  "keywords": ["http-foundation", "image", "http"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "iwyg",
      "email": "mail@thomas-appel.com"
    }
  ],
  "require": {
      "thapp/jmg": "dev-develop",
      "symfony/http-foundation": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Thapp\\Jmg\\Http\\Foundation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thapp\\Jmg\\Tests\\Http\\Foundation\\": "tests/"
    }
  },
  "suggest": {
    "symfony/http-kernel":"to utilize kernel http exceptions."
  },
  "minimum-stability": "dev"
}
